{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true
    },
    "extends" : ["airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": ["/src/main/resources/static/*.js"],
    "rules": {
        "no-var": "warn",
        "no-unused-vars": "warn",
        "prefer-const": "off",
        "prettier/prettier": [
          "warn",
          {
            "endOfLine": "auto"
          }
        ]
      }
}
